{
    "name": "bangron-work/pocketdb",
    "description": "MongoDB-like interface to SQLite",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rony Herdian",
            "email": "herdianrony@gmail.com"
        }
    ],
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "PocketDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}